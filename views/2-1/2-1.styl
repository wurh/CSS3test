*
  margin 0
  padding 0
.clearfix:after,.clearfix:before
  display table
  content ""
.clearfix:after
  clear both
  overflow hidden

.demo
  width 250px
  border 1px solid #ccc
  padding 10px
  margin 20px auto

li
  list-style none outside none
  float left
  height 20px
  line-height 20px
  width 20px
  border-radius 10px
  text-align center
  background #f36
  color green
  margin-right 5px
